{
  "name": "@consonantlabs/control-plane",
  "version": "1.0.0",
  "description": "Consonant Labs Control Plane - Production SaaS Infrastructure",
  "main": "dist/server.js",
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm run prisma:generate && nodemon --exec node --import tsx src/server.ts",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/server.js",
    "test": "NODE_ENV=test vitest",
    "test:watch": "NODE_ENV=test vitest --watch",
    "test:coverage": "NODE_ENV=test vitest --coverage",
    "prisma:sync": "node prisma/sync-provider.js",
    "prisma:generate": "npm run prisma:sync && prisma generate",
    "prisma:migrate": "npm run prisma:sync && prisma migrate dev",
    "prisma:migrate:deploy": "npm run prisma:sync && prisma migrate deploy",
    "prisma:migrate:create": "npm run prisma:sync && prisma migrate dev --create-only",
    "prisma:reset": "npm run prisma:sync && prisma migrate reset --force",
    "prisma:push": "npm run prisma:sync && prisma db push",
    "prisma:pull": "npm run prisma:sync && prisma db pull",
    "prisma:validate": "npm run prisma:sync && prisma validate",
    "prisma:studio": "npm run prisma:sync && prisma studio",
    "prisma:format": "npm run prisma:sync && prisma format",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "control-plane",
    "saas",
    "observability",
    "telemetry",
    "fastify",
    "prisma",
    "grpc",
    "timescaledb"
  ],
  "author": "Consonant Labs",
  "license": "UNLICENSED",
  "dependencies": {
    "@fastify/compress": "^8.3.1",
    "@fastify/cors": "^11.2.0",
    "@fastify/env": "^5.0.3",
    "@fastify/helmet": "^13.0.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/request-context": "^6.2.1",
    "@grpc/grpc-js": "^1.14.3",
    "@grpc/proto-loader": "^0.7.14",
    "@libsql/client": "^0.15.15",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.53.0",
    "@opentelemetry/core": "^1.25.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.53.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.53.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.53.0",
    "@opentelemetry/instrumentation-fastify": "^0.41.0",
    "@opentelemetry/instrumentation-http": "^0.53.0",
    "@opentelemetry/instrumentation-ioredis": "^0.44.0",
    "@opentelemetry/instrumentation-pg": "^0.47.0",
    "@opentelemetry/instrumentation-pino": "^0.43.0",
    "@opentelemetry/resources": "^1.25.0",
    "@opentelemetry/sdk-logs": "^0.53.0",
    "@opentelemetry/sdk-metrics": "^1.25.0",
    "@opentelemetry/sdk-node": "^0.53.0",
    "@opentelemetry/sdk-trace-base": "^1.25.0",
    "@opentelemetry/sdk-trace-node": "^1.25.0",
    "@opentelemetry/semantic-conventions": "^1.27.0",
    "@prisma/adapter-libsql": "^7.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "async-mutex": "^0.5.0",
    "bcrypt": "^6.0.0",
    "close-with-grace": "^2.3.0",
    "dotenv": "^16.5.0",
    "fastify": "^5.6.2",
    "fastify-plugin": "^5.1.0",
    "inngest": "^3.48.1",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.13.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.3",
    "uuid": "^11.0.6",
    "zod": "^4.1.13",
    "@fastify/passport": "^3.0.0",
    "@fastify/secure-session": "^8.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-headerapikey": "^1.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^24.10.4",
    "@types/pg": "^8.11.11",
    "@typescript-eslint/eslint-plugin": "^8.28.2",
    "@typescript-eslint/parser": "^8.28.2",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "nodemon": "^3.1.11",
    "prettier": "^3.5.3",
    "prisma": "^7.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.1",
    "@vitest/coverage-v8": "^3.2.1",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "@types/passport": "^1.0.17",
    "@types/passport-jwt": "^4.0.1"
  }
}